<h1>Restaurants#index</h1>
<p>Find me in app/views/restaurants/index.html.erb</p>
<ul>
  <% @restaurants.each do |restaurant| %>
  <li> <h2> <%= restaurant.name %> <h2> </li>
    <%= link_to 'view details', restaurant_path(restaurant), class: "btn btn-primary" %>
    <%= link_to 'add review', new_restaurant_review_path(restaurant), class: "btn btn-primary" %>
  <% end %>
</ul>
